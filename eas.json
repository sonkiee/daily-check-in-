{
  "cli": {
    "version": ">= 16.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://lucky-busy-aphid.ngrok-free.app/api/v1"
      },
      "ios": {
        "simulator": true
      }
    },
    "simulator": {
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env":  {
        "EXPO_PUBLIC_API_URL": "https://checkin-backend-qohy.onrender.com/api/v1"
        
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env":  {
        "EXPO_PUBLIC_API_URL": "https://checkin-backend-qohy.onrender.com/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
